				<ui:include src="/administration/alertReceiverSetup.jsf"></ui:include>
			</rich:tab>
			<rich:tab id="emailServerTab" label="Email Servers">
				<ui:include src="/administration/emailServers.jsf"></ui:include>
			</rich:tab>
			<rich:tab id="snmpDestinationTab" label="SNMP Destinations">
				<ui:include src="/administration/snmpDestinations.jsf"></ui:include>
			</rich:tab>
		</rich:tabPanel>

	</a4j:outputPanel>
</ui:composition>