<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>
		
	<managed-bean>
		<managed-bean-name>userMBean</managed-bean-name>
		<managed-bean-class>org.eurekaj.manager.managed.UserMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>treeMenuMBean</managed-bean-name>
		<managed-bean-class>org.eurekaj.manager.managed.TreeMenuMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>treeMenuService</property-name>
			<property-class>org.eurekaj.manager.service.TreeMenuService</property-class>
			<value>#{treeMenuService}</value>
		</managed-property>
		<managed-property>
			<property-name>userMBean</property-name>
			<property-class>org.eurekaj.manager.managed.UserMBean</property-class>
			<value>#{userMBean}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>chartMBean</managed-bean-name>
		<managed-bean-class>org.eurekaj.manager.managed.ChartMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>treeMenuService</property-name>
			<property-class>org.eurekaj.manager.service.TreeMenuService</property-class>
			<value>#{treeMenuService}</value>
		</managed-property>
		<managed-property>
			<property-name>userMBean</property-name>
			<property-class>org.eurekaj.manager.managed.UserMBean</property-class>
			<value>#{userMBean}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>navigationMbean</managed-bean-name>
		<managed-bean-class>org.eurekaj.manager.managed.NavigationMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>treeMenuService</property-name>
			<property-class>org.eurekaj.manager.service.TreeMenuService</property-class>
			<value>#{treeMenuService}</value>
		</managed-property>
		<managed-property>
			<property-name>userMBean</property-name>
			<property-class>org.eurekaj.manager.managed.UserMBean</property-class>
			<value>#{userMBean}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>groupInstrumentationMbean</managed-bean-name>
		<managed-bean-class>org.eurekaj.manager.managed.GroupInstrumentationMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>treeMenuService</property-name>
			<property-class>org.eurekaj.manager.service.TreeMenuService</property-class>
			<value>#{treeMenuService}</value>
		</managed-property>
		<managed-property>
			<property-name>userMBean</property-name>
			<property-class>org.eurekaj.manager.managed.UserMBean</property-class>
			<value>#{userMBean}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>alertMBean</managed-bean-name>
		<managed-bean-class>org.eurekaj.manager.managed.AlertMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>treeMenuService</property-name>
			<property-class>org.eurekaj.manager.service.TreeMenuService</property-class>
			<value>#{treeMenuService}</value>
		</managed-property>
		<managed-property>
			<property-name>userMBean</property-name>
			<property-class>org.eurekaj.manager.managed.UserMBean</property-class>
			<value>#{userMBean}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>administrationMBean</managed-bean-name>
		<managed-bean-class>org.eurekaj.manager.managed.AdministrationMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>administrationService</property-name>
			<property-class>org.eurekaj.manager.service.AdministrationService</property-class>
			<value>#{administrationService}</value>
		</managed-property>
	</managed-bean>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>navigateTo.instrumentation</from-outcome>
			<to-view-id>/index.jsf</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigateTo.groupInstrumentation</from-outcome>
			<to-view-id>/groupInstrumentation.jsf</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>

	<lifecycle/>
		<!--phase-listener>net.sf.jsfcomp.chartcreator.ChartListener</phase-listener>
	</lifecycle-->
	<application>
		<variable-resolver>
			org.springframework.web.jsf.DelegatingVariableResolver
		</variable-resolver>
		<locale-config>
			<default-locale>no</default-locale>
		</locale-config>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
	</application>
</faces-config>